{
  "manifest_version": 2,
  "name": "ArcFox",
  "version": "3.0.0",
  "description": "Make your Firefox like the Arc Browser.",
  "icons": {
    "16": "./src/assets/arcfox-default.svg",
    "48": "./src/assets/arcfox-square-transparent.svg",
    "96": "./src/assets/arcfox-default.svg"
  },
  "background": {
    "scripts": [
      "./src/background/keybinds.js"
    ]
  },
  "commands": {
    "toggleSidebar": {
      "suggested_key": {
        "default": "Alt + S"
      },
      "description": "Toggle extension sidebar."
    }
  },
  "sidebar_action": {
    "default_panel": "src/sidebar/index.html"
  }
}